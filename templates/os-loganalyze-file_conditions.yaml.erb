conditions:
 - filename_pattern: ^.*\.txt\.gz$
   filter: SevFilter
   view: HTMLView
 - filename_pattern: ^.*\.txt?$
   filter: SevFilter
   view: TextView
 - filename_pattern: ^.*\.html\.gz$
   filter: SevFilter
   view: HTMLView
 - filename_pattern: ^.*\.html?$
   filter: SevFilter
   view: TextView
 - filename_pattern: ^.*$
   filter: NoFilter
   view: PassthroughView
